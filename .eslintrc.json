{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "linebreak-style": 0,
    "no-lonely-if": 0,
    "no-return-assign": 0,
    "no-new": 0,
    "no-param-reassign": 0,
    "no-unused-expressions": 0,
    "no-underscore-dangle": 0,
    "no-global-assign": 0,
    "consistent-return": 0
  }
}
